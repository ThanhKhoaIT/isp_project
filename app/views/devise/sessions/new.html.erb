<div id="login">
  <%= form_for(resource, as: resource_name, url: "/users/sign_in", method: "POST") do |f| %>
    <fieldset class="clearfix">
      <p>
				<span class="fontawesome-user"></span>
				<%= f.email_field :email, autofocus: true, required: "required", placeholder: "Email"%>
			</p>
      <p>
				<span class="fontawesome-lock"></span>
				<%= f.password_field :password, autocomplete: "off", required: 'required', placeholder: "Password" %>
			</p>
      <p><input type="submit" value="Login"></p>
    </fieldset>
  <% end %>
  <%= render "devise/shared/links" %>
</div>